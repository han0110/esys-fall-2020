{
    "parser": "babel-eslint",
    "extends": [
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}